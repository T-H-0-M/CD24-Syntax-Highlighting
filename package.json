{
  "name": "cd24-syntax-highlighting",
  "displayName": "CD24-Syntax-Highlighting",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "cd24",
        "aliases": [
          "CD24",
          "cd24"
        ],
        "extensions": [
          ".cd"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "cd24",
        "scopeName": "source.cd24",
        "path": "./syntaxes/cd24.tmLanguage.json"
      }
    ]
  },
  "semanticTokenTypes": [
    {
      "id": "customType",
      "superType": "type",
      "description": "Custom type in CD24"
    }
  ],
  "semanticTokenModifiers": [],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
